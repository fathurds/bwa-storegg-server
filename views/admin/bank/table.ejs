<table id="example2" class="table table-bordered table-hover">
    <thead>
    <tr>
      <th>No.</th>
      <th>Nama Pemilik</th>
      <th>Nama Bank</th>
      <th>No. Rekening</th>
      <th>Aksi</th>
    </tr>
    </thead>
    <tbody>
      <% for(let i = 0; i < bank.length; i++){ %>
        <tr>
            <td><%= i + 1 %></td>
            <td><%= bank[i].name %></td>
            <td><%= bank[i].bankName %></td>
            <td><%= bank[i].noRekening %></td>
            <td>
            <form action="/bank/delete/<%= bank[i]._id %>?_method=DELETE" method="post">
                <a href="/bank/edit/<%= bank[i]._id %>" class="btn btn-info">
                <li class="fas fa-pencil-alt"></li>
                Ubah
                </a>
                <button class="btn btn-danger">
                <li class="fas fa-trash"></li>
                Hapus
                </button>
            </form>
            </td>
        </tr>
    <% } %>
  </table>