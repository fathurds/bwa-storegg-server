<table id="example2" class="table table-bordered table-hover">
    <thead>
    <tr>
      <th>No.</th>
      <th>Nama Game</th>
      <th>Kategori</th>
      <th>Nominal</th>
      <th>Status</th>
      <th>Aksi</th>
    </tr>
    </thead>
    <tbody>
      <% for(let i = 0; i < voucher.length; i++){ %>
        <tr>
            <td><%= i + 1 %></td>
            <td><%= voucher[i].name %></td>
            <td><%= voucher[i]?.category?.name ?? '-' %></td>
            <td>
                <% voucher[i].nominals.forEach(nominal => { %>
                    <span class="badge badge-warning">
                        <%= nominal.coinName %> <%= nominal.coinQuantity %>
                    </span>
                <% }) %>
            </td>
            <td>
                <% if(voucher[i].status == 'Y') { %>
                    <span class="badge badge-success">
                        Aktif
                    </span>
                <% } else { %>
                    <span class="badge badge-danger">
                        Tidak Aktif
                    </span>
                <% } %>
            </td>
            <td>
                <form action="/voucher/status/<%= voucher[i]._id %>?_method=PUT" method="post" style="display:inline-block">
                    <button class="btn btn-primary">
                        <li class="fas fa-folder"></li>
                            <% if(voucher[i].status == 'Y') { %>
                                Matikan
                            <% } else { %>
                                Hidupkan
                            <% } %>
                        </button>
                    </form>
                </form>
            <form action="/voucher/delete/<%= voucher[i]._id %>?_method=DELETE" method="post" style="display:inline-block">
                <a href="/voucher/edit/<%= voucher[i]._id %>" class="btn btn-info">
                <li class="fas fa-pencil-alt"></li>
                Ubah
                </a>
                <button class="btn btn-danger">
                <li class="fas fa-trash"></li>
                Hapus
                </button>
            </form>
            </td>
        </tr>
    <% } %>
  </table>